            <field name="model">account.custom.entry.type</field>
            <field name="inherit_id" ref="de_custom_journal_entry.account_custom_entry_type_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//group[1]/group[2]" position="inside">
                    <field name="has_spare_penalty" widget="radio" options="{'horizontal': true}"/>
